<?xml version="1.0" encoding="UTF-8"?>
<clientConfiguration xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews
http://fews.wldelft.nl/schemas/version1.0/clientConfig.xsd">
	<clientType>Stand alone</clientType>
	<jvmOption>-Xms512m</jvmOption>
	<jvmOption>-Xmx2048m</jvmOption>
	<jvmOption>-DautoRollingBarrel=false</jvmOption>
	<localCacheSizeMB>2000</localCacheSizeMB>
	<logging>
		<debugEnabled>false</debugEnabled>
	</logging>
	<autoExportModuleDataSet name="Products" exportDir="Products"/>
	<autoExportModuleDataSet name="Import_preprocessing" exportDir="Modules/Import_preprocessing"/>
		<autoExportModuleDataSet name="Export_postprocessing" exportDir="Modules/Export_postprocessing"/>

</clientConfiguration> 
