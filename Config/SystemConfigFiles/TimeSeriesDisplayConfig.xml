<timeSeriesDisplay xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/timeSeriesDisplay.xsd">
	<generalDisplayConfig>
		<convertDatum>true</convertDatum>
		<valueEditorPermission>bewerken</valueEditorPermission>
		<labelEditorPermission>bewerken</labelEditorPermission>
		<commentEditorPermission>bewerken</commentEditorPermission>
	</generalDisplayConfig>
	<defaultViewPeriod unit="day" start="-365" end="0"/>
	<timeMarkersDisplayConfig>
		<timeMarkerDisplayOptions marker="systemTime">
			<color>green</color>
			<lineStyle>solid</lineStyle>
			<label>Systeem Tijd</label>
		</timeMarkerDisplayOptions>
		<timeMarkerDisplayOptions marker="timeZero">
			<color>gray</color>
			<lineStyle>dotted</lineStyle>
			<label>T0</label>
		</timeMarkerDisplayOptions>
		<timeMarkerDisplayOptions marker="displayTime">
			<color>red</color>
			<lineStyle>solid</lineStyle>
			<label>Huidige Tijd</label>
		</timeMarkerDisplayOptions>
		<timeMarkerDisplayOptions marker="threshold">
			<color>orange</color>
			<lineStyle>dashed</lineStyle>
			<label>Waarschuwings Limiet</label>
		</timeMarkerDisplayOptions>
		<timeMarkerDisplayOptions marker="forecastConfidence1">
			<color>purple</color>
			<lineStyle>dashed</lineStyle>
			<label>Betrouwbaarheid Periode 1</label>
		</timeMarkerDisplayOptions>
		<timeMarkerDisplayOptions marker="forecastConfidence2">
			<color>black</color>
			<lineStyle>dashed</lineStyle>
			<label>Betrouwbaarheid Periode 2</label>
		</timeMarkerDisplayOptions>
		<timeMarkerDisplayOptions marker="forecastConfidence3">
			<color>purple</color>
			<lineStyle>dashed</lineStyle>
			<label>Betrouwbaarheid Periode 3</label>
		</timeMarkerDisplayOptions>
	</timeMarkersDisplayConfig>
	<parametersDisplayConfig>
		<defaults>
			<preferredColor>blue</preferredColor>
			<lineStyle>solid</lineStyle>
			<precision>10</precision>
		</defaults>
		<parameterDisplayOptions id="AANTL[n]">
			<preferredColor>blue</preferredColor>
			<lineStyle>solid</lineStyle>
			<precision>10</precision>
		</parameterDisplayOptions>
	</parametersDisplayConfig>
	<moduleInstanceIdMappings>
		<moduleInstanceIdMapping id="Import_IMmetingen">
			<description>Sample data</description>
		</moduleInstanceIdMapping>
	</moduleInstanceIdMappings>
