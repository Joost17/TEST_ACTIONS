	<parameterRootNode id="Parameters">
		<parameterNode id="1. Veldmetingen en -waarnemingen">
			<parameterNode id="Veld biologisch">
				<constraints>
					<attributeTextEquals id="group" equals="Veld biologisch"/>
				</constraints>
			</parameterNode>
			<parameterNode id="Veld fysisch-chemisch">
				<constraints>
					<attributeTextEquals id="group" equals="Veld fysisch-chemisch"/>
				</constraints>
			</parameterNode>
		</parameterNode>
		<parameterNode id="2. Fysisch-chemisch">
			<parameterNode id="Algemeen fysisch-chemisch">
				<parameterNode id="Nutriënten">
					<constraints>
						<attributeTextEquals id="group" equals="Nutriënten"/>
					</constraints>
				</parameterNode>
				<parameterNode id="Overig fysisch-chemisch">
					<constraints>
						<attributeTextEquals id="group" equals="Overig fysisch-chemisch"/>
					</constraints>
				</parameterNode>
				<parameterNode id="Blauwalgen">
					<constraints>
						<attributeTextEquals id="group" equals="Blauwalgen"/>
					</constraints>
				</parameterNode>
				<parameterNode id="Bacteriologisch">
					<constraints>
						<attributeTextEquals id="group" equals="Bacteriologisch"/>
					</constraints>
				</parameterNode>
			</parameterNode>
			<parameterNode id="Metalen">
				<constraints>
					<attributeTextEquals id="group" equals="Metalen"/>
				</constraints>
			</parameterNode>
			<parameterNode id="Organische microverontreinigingen">
				<parameterNode id="Bestrijdingsmiddelen">
					<constraints>
						<attributeTextEquals id="group" equals="Bestrijdingsmiddelen"/>
					</constraints>
				</parameterNode>
				<parameterNode id="Organotin verbindingen">
					<constraints>
						<attributeTextEquals id="group" equals="Organotin verbindingen"/>
					</constraints>
				</parameterNode>
				<parameterNode id="Geneesmiddelen">
					<constraints>
						<attributeTextEquals id="group" equals="Geneesmiddelen"/>
					</constraints>
				</parameterNode>				
				<parameterNode id="Overige organische microverontreinigingen">
					<constraints>
						<attributeTextEquals id="group" equals="Overige organische microverontreinigingen"/>
					</constraints>
				</parameterNode>
				<parameterNode id="Polyaromatische koolwaterstoffen (PAK)">
					<constraints>
						<attributeTextEquals id="group" equals="Polyaromatische koolwaterstoffen (PAK)"/>
					</constraints>
				</parameterNode>
			</parameterNode>
		</parameterNode>
		<parameterNode id="3. Biologisch">
			<constraints>
				<anyValid>
					<attributeTextEquals id="group" equals="3. Biologisch"/>
				</anyValid>
			</constraints>
		</parameterNode>
		<parameterNode id="4. Toetsresultaten (per jaar)">
			<constraints>
				<attributeTextEquals id="group" equals="4. Toetsresultaten (per jaar)"/>
			</constraints>
		</parameterNode>
		<parameterNode id="5. Toestandsbeoordeling (meerjarig)">
			<constraints>
				<attributeTextEquals id="group" equals="5. Toestandsbeoordeling (meerjarig)"/>
			</constraints>
		</parameterNode>
		<parameterNode id="Overig">
			<constraints>
				<not>
					<attributeExists id="group"/>
				</not>
			</constraints>
		</parameterNode>
	<parameterNode id="6. Afgemeld">
			<constraints>
				<attributeTextEquals id="group" equals="6. Afgemeld"/>
			</constraints>
		</parameterNode>		
		<parameterNode id="7. Niet bemeten">
			<constraints>
				<attributeTextEquals id="group" equals="7. Niet bemeten"/>
			</constraints>
		</parameterNode>
	</parameterRootNode>
</parameters>
